{
  "name": "backend",
  "version": "1.0.0",
  "description": "Express.js backend with TypeORM, tRPC, and authentication",
  "main": "dist/index.js",
  "workspaces": [
    "backend"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "debug": "NODE_OPTIONS='--enable-source-maps' nodemon --inspect src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "seed": "tsx src/database/seeds/admin.seed.ts",
    "seed:admin": "tsx src/database/seeds/admin.seed.ts",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:drop": "drizzle-kit drop",
    "migration:generate": "npm run drizzle:generate",
    "migration:run": "npm run drizzle:migrate",
    "migration:push": "npm run drizzle:push"
  },
  "dependencies": {
    "@better-auth/utils": "^0.2.6",
    "@trpc/server": "^10.45.2",
    "@types/pg": "^8.15.5",
    "@types/sqlite3": "^3.1.11",
    "bcryptjs": "^3.0.2",
    "better-auth": "^1.3.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.44.5",
    "drizzle-zod": "^0.8.3",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.4",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.1",
    "drizzle-kit": "^0.31.4",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "express",
    "typescript",
    "typeorm",
    "trpc",
    "authentication",
    "database-agnostic"
  ],
  "author": "kalebademkisho@gmail.com",
  "license": "MIT"
}
